<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="1,2">
        <GradientStop Color="#B7E0FF" Offset="0.0"/>
        <GradientStop Color="#FFF5CD" Offset="1.0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="cs_BorderBrush" Color="#FFF5CD"/>
    <SolidColorBrush x:Key="cs_BBorderBrush" Color="#FFEDDCEB" Opacity="0.5" />
    <SolidColorBrush x:Key="cs_Text" Color="#808080"  />
    <SolidColorBrush x:Key="cs_Text2" Color="#808080"  />
    <SolidColorBrush x:Key="cs_Text3" Color="#808080"  />
    <SolidColorBrush x:Key="cs_TextBrush" Color="#FFF5CD"  />


</ResourceDictionary>